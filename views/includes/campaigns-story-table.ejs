<script>
    function toggleDetails(div_id) {
        console.log(div_id);
        let detailsDiv = document.getElementById(div_id);
        if (detailsDiv.style.display === "none") {
            detailsDiv.style.display = "block";
        } else {
            detailsDiv.style.display = "none";
        }
    }
</script>
